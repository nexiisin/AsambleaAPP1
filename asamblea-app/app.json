{
  "expo": {
    "name": "asamblea-app",
    "slug": "asamblea-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-logo.png",
    "scheme": "asambleaapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.asambleaapp"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#5fba8b",
        "foregroundImage": "./assets/images/android-icon-foreground.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.asambleaapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/splash-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-logo.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#5fba8b",
          "dark": {
            "image": "./assets/images/splash-logo.png",
            "backgroundColor": "#5fba8b"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
